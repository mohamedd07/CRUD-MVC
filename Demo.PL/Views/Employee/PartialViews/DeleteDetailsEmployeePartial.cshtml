@model EmployeeViewModel




<img src="~/files/images/@Model.ImageName" width="75" class="img-fluid rounded-circle" alt="Employee" />
<div class="form-group">

    <label class="col-form-label" asp-for="Name"> @*Code*@ </label>
    <input asp-for="Name" class="form-control" readonly />



</div>

<div class="form-group">

    <label class="col-form-label" asp-for="Age"> @*Code*@ </label>
    <input asp-for="Age" class="form-control" readonly />


</div>

<div class="form-group">

    <label class="col-form-label" asp-for="Address"> @*Code*@ </label>
    <input asp-for="Address" class="form-control" readonly />



</div>



<div class="form-group">

    <label class="col-form-label" asp-for="Salary"> @*Code*@ </label>
    <input asp-for="Salary" class="form-control" readonly />


</div>



<div class="form-check">


    <input asp-for="IsActive" class="form-check-input" readonly />
    <label class="col-form-label" asp-for="IsActive"> @*Code*@ </label>



</div>


<div class="form-group">

    <label class="col-form-label" asp-for="Email"> @*Code*@ </label>
    <input asp-for="Email" class="form-control" readonly />


</div>

<div class="form-group">

    <label class="col-form-label" asp-for="PhoneNumber"> @*Code*@ </label>
    <input asp-for="PhoneNumber" class="form-control" readonly />


</div>

<div class="form-group">

    <label class="col-form-label" asp-for="HireDate"> @*Code*@ </label>
    <input asp-for="HireDate" class="form-control" readonly />



</div>



